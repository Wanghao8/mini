/* 底部按钮 */
.back {position: fixed;bottom: 0;width: 100%;}
.backHome {height: 100rpx;line-height: 100rpx;width: 100%;font-size: 32rpx;border-radius: 0;text-align: center;}
.backIcon{width: 100%;}
.phoneBtn {height: 100rpx;line-height: 100rpx;width: 16%;background-color: #666666;}
.phone {font-size: 50rpx;text-align: center;line-height: 100rpx;color: #CCCCCC;}
