.map{text-decoration: underline}
.card1{width: 710rpx;border-radius: 10rpx;overflow: hidden}
.icon-jinggao1{margin: 20rpx;font-size: 44rpx;color: #97DDD6}
.bar{width: 630rpx;height: 80rpx;background-color: #242424}
.pad{padding-bottom: 20rpx}
.icon-fenxiang{font-size: 40rpx;margin-right: -4rpx;color: #333;}
.img-list{ margin-left: -50rpx;}
.img-list::after{clear: both;}
.img-list image{float: left;width: 100rpx;height: 100rpx;margin-left:20rpx;margin-bottom: 20rpx;border-radius: 50%;border: 1px solid #cccccc;
}
page>scroll-view{height: 100vh}