
.main{
  margin: 0 30rpx;
}
.title{
  font-size: 32rpx;text-align: right;color: #999;margin: 20rpx 0;
}
.card_img{
  border-radius: 30rpx;	box-shadow: 0 6px 12px 0 rgba(0,0,0,.15);
}

.relative{margin: 30rpx;padding-bottom: 30rpx;}





.number{
	padding: 10rpx 0 30rpx;
	margin: 0 30rpx;
	position: relative;
	overflow: hidden;
}
.number-l{
	float: left;
}
.number-r{
	float: right;
font-size: 40rpx;
}
.number-r text{
	font-size: 44rpx;
}
.number-r span{
	padding: 0 30rpx;
}
.type{
	flex: 1;
	text-align: center;
}
.sele{
	font-size: 32rpx;
	color: #fff;
	background-color: #26CD91;
	padding: 10rpx 30rpx;
	border-radius: 10rpx;
}
.unsele{
	font-size: 32rpx;
	color: #26CD91;
	border: 1rpx solid #26CD91;
	padding: 10rpx 30rpx;
	border-radius: 10rpx;
}
.tipsinfo{
	position: fixed;
	top:50%;
    left:50%;
	width: 70%;
    transform:translate(-50%,-50%);
	background-color: #fff;
	z-index: 99;
	border-radius: 30rpx;
	font-size: 32rpx;
	padding: 30rpx;
}
.tipsinfo .icon{
  color: #ccc;font-size: 36rpx; position: absolute;right: 0;top: -50rpx;
}
.tipsinfo .tan_title{
  text-align: center;font-size: 38rpx;margin-bottom: 40rpx;
}
.tipsinfo .tan_btn{
  width: 80%;background-color: #26CD91;color: #fff;line-height: 80rpx;margin-top: 20rpx;
}
.modelInfo{font-size: 30rpx;margin: 30rpx;}


.list{
  display: flex;flex-direction: row;
}
.list .money{
  font-size: 32rpx;margin-top: 14rpx;
}

.btn_box{
  position: fixed;bottom: 30rpx;width: 92%;margin: 0 30rpx;
}
.btn_box button{
  background-color: #26CD91;color: #fff;
}













