page{background-color: #eee;}
.footprint{position: relative;background-color: #fff;display: flex;flex-direction: row;font-size: 32rpx;}
.footprint-l{flex: 3;overflow: hidden;padding-bottom: 30rpx;}
.footprint-l image {width: 160rpx;height: 160rpx;margin: 36rpx 0 0 30rpx;}
.footprint-r{flex: 7;margin: 30rpx 30rpx 0 0;}
.footprint-l-t{overflow: hidden;font-size: 32rpx;}
.footprint-l-b{font-size: 30rpx;color: #999;padding: 8rpx 0;}
.footprint-r-t{float: right;font-size: 30rpx;color: #999;}
.footprint-r-b{float: left; font-size: 30rpx;color: #999;}
.graceLoading{margin:20rpx 0;}
.thinline{background-color: #fff;padding: 0 30rpx 30rpx;font-size: 32rpx;}