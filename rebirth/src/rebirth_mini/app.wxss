.gym-wrapper{width: 690rpx;margin: 30rpx;}
.jump-bar{width: 690rpx;height: 120rpx;line-height: 120rpx; border-radius: 10rpx}
.gym-card{height: 400rpx;width: 690rpx;border-radius: 20rpx;margin-bottom: 30rpx;}
.gym-card:last-child{margin-bottom: 0}
.page-wrapper{width: 690rpx;padding: 0 30rpx;}

.card-list-item{margin-bottom: 30rpx;}
.bottom-btn{position: fixed;bottom: 60rpx;height:130rpx;width:690rpx;text-align: center; line-height: 130rpx;border-radius: 20rpx;  background-color: #FCC800;color: #333;}
.navigator{position: sticky;top: 0; height: 200rpx;line-height: 200rpx;background-color: #242424;}
.little-btn{width: 140rpx;height: 70rpx;background-color: #FCC800;border-radius: 12rpx;color: #333}
.avatar-wrapper{height: 200rpx;width: 200rpx;border-radius: 50%;}
.card{height: 250rpx;border-radius: 20rpx;}
.concise-content{height: 150rpx;}
.ellipsis-triangle{height: 60rpx;text-align: center;}
.date-circle{width: 80rpx;height: 80rpx;border-radius: 50%;}
.date-circle-active{border: 1px solid #FCC800;background-color: #242424;color: #fff;}

.list-warpper{background-color: #333;border-radius: 20rpx;}
.list-item{position: relative;height: 200rpx;}
.list-item::after,.options::after{position: absolute;content: '';-webkit-transform-origin: 0 0;transform-origin: 0 0;-webkit-transform: scaleY(0.5);transform: scaleY(0.5);bottom: 0;right: 0;width: 100%;border-bottom: 1px solid #39393A;}
.coach-img{height: 140rpx;width: 140rpx;border-radius: 50%}
.tag{height: 60rpx;width:fit-content;padding:0 20rpx;border-radius: 60rpx;background-color: #555555;color: #fff}
.class-img{width: 340rpx;height: 160rpx;border-radius: 4rpx;}
.options{position: relative;height: 120rpx;}
.mine-card{height: 400rpx;border: 1px solid #797979;width: 100%}

/* 颜色 */
.white{color: #fff}
.libg{background-color: #333}
.txtcol{color: #ababab}
.bg24{background-color: #242424}
