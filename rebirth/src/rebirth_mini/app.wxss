.yellowbg{background-color: #FCC800}

/* 间距 */
.margin-row-15{margin: 0 30rpx;}
.marginleft5{margin-left: 10rpx}
.marginleft10{margin-left: 20rpx}
.margintop10{margin-top: 20rpx}

/* 字体大小 */
