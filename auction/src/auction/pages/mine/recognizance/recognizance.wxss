.header{position: fixed;top:0; padding: 40rpx 0;border: 1px solid #E6E6E6;width: 100%;z-index: 10}
.tabitem{width: 25%;text-align: center;color: #999}
.actived{font-weight: 600;color: #000}
.iconicon-test{font-size: 100rpx;color:#D8D8D8; }
.content-wrapper{height: 80vh}
.tip{margin-top: 40rpx;color:#D8D8D8;}
.con-wrapper{margin-top: 140rpx;}
.listtitle{padding: 40rpx 30rpx;border-bottom: 1px solid #E6E6E6;}
.iconyoujiantou{font-size: 24rpx;color:#B6B6B6 }
.pay-wrapper{border-bottom: 1px solid #E6E6E6;}
.paytitle{font-weight: 600;margin: 30rpx 30rpx 10rpx 30rpx;font-size: 30rpx}
.paytime{margin: 0 0 30rpx 30rpx}
.pay,.refund{margin-right: 30rpx;font-weight: 600;font-size: 30rpx}
.refund{color: #34AC16}
.locked{opacity: 0.6}
.bottomtips{position: relative;width: 100%;text-align: center;margin-top: 40rpx}

