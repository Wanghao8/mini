.shadow{position: absolute;width: 100%;top: 0;	bottom: 0; height: 400%;z-index: 1;background: rgba(0,0,0,.5);}
.shadow2{position: absolute;width: 100%;top: 0;bottom: 0;height: 200%;z-index: 3;background: rgba(0,0,0,.5);}
.countdown{position: fixed;bottom: 150rpx; width: 300rpx;height: 80rpx;z-index: 5;border-radius: 40rpx;border: 1px solid #E6E6E6;padding-left: 40rpx;margin: 0 0 20rpx 20rpx;}
