page{background-color: #F1F1F1;padding-bottom: 700rpx;}
.loadinggif{width: 80rpx;height: 80rpx;margin-bottom: 40rpx;color: #666666}
swiper,swiper-item{height: 750rpx!important}
.img{height: 750rpx;width: 100%}
.header{background-color: #ffffff;overflow: hidden;position: relative}
.header::after{position: absolute;content: '';-webkit-transform-origin: 0 0;transform-origin: 0 0;-webkit-transform: scaleY(0.5);transform: scaleY(0.5);bottom: 0;right: 0;width: 100%;border-bottom: 1px solid #E6E6E6;}
.avatar-wrapper{margin: 40rpx 0 20rpx 40rpx}
.avatar{height: 50rpx;width: 50rpx;border-radius: 50%;}
.nickname{margin: 8rpx 0 0 10rpx;}
.bluev{background-color: #4DA4CE;color: #fff;width: 30rpx;height: 30rpx;border-radius: 50%;font-size: 30rpx;text-align: center;line-height: 24rpx;}
.tags{margin-right: 20rpx;}
.tag{border: 1px solid #DEDEDE;height: 40rpx;line-height: 40rpx;text-align: center;padding: 6rpx 10rpx;border-radius: 6rpx;margin-left: 12rpx}
.name{font-weight: 800;margin: 0rpx 40rpx 20rpx 40rpx}
.time{margin: 0 0 40rpx 40rpx}
.remind,.share{width: 50%;border: none;border-radius: 0;line-height: 100rpx;text-align: center}
.remind{position: relative}
.remind::after{position: absolute;content: '';-webkit-transform-origin: 0 0;transform-origin: 0 0;-webkit-transform: scaleX(0.5);transform: scaleX(0.5);top: 0;right: 0;height: 100%;border-right: 1px solid #e5e5e5; }
.share::after{border: none}
.iconiconjia,.iconshare,.icontixing{padding-right: 10rpx}
button::after{content: "";border: none;border-radius: 0}
.price-wrapper{background-color: #ffffff}
.content-wrapper{width: 750rpx}
.left,.right{width: 50%}
