
page{background-color: #F1F1F1;padding-bottom: 400rpx}
.header,.middle,.footer {background-color: #ffffff;overflow: hidden}
.name{position: relative; padding: 20rpx 0;margin-right: 40rpx;height: 50rpx}
.name::after{position: absolute;content: '';-webkit-transform-origin: 0 0;transform-origin: 0 0;-webkit-transform: scaleY(0.5);transform: scaleY(0.5);bottom: 0;right: 0;width: 100%;border-bottom: 1px solid #E6E6E6;}
.descr{padding: 20rpx 0;width: 95%;height: 120rpx}
.imglist{margin: 10rpx}
.question-images{height: 100%;}
.q-image-wrap{padding-bottom: 26rpx}
.q-image{width: 180rpx;height: 180rpx}
.q-image-remover{position: relative; width: 30rpx;height: 30rpx;border-radius: 50%;color: #fff;background-color: red;font-weight: 2000;text-align: center;line-height: 20rpx;margin-left: 180rpx;font-size: 50rpx;top: 15rpx;border: 1px solid #fff}
.addimg{width: 180rpx;height: 180rpx;background-color: #F5F5F5;text-align: center;margin: 20rpx}
.plus{font-size: 100rpx;line-height: 180rpx;font-weight: 100;color: #B2B2B2}
.enter{position: fixed;bottom: 0;height: 100rpx;line-height: 100rpx;text-align: center;color:#F66F36;  z-index: 100;background-color: #fff;width: 100%}
.listItem{height: 100rpx; border-bottom: 1px solid #E6E6E6;padding-left: 20rpx}
.iconfont{font-size: 40rpx;color: #B3B3B3; margin:20rpx}
.start_time,.end_time,.delaytime{width: 50rpx;height: 60rpx;line-height: 60rpx;text-align: right;}
.subtxt{height: 60rpx;line-height: 60rpx}
.arrow{margin-right: 20rpx;}
.iconyoujiantou{font-size: 30rpx}
.last{border: none}
.bottombtn{position: fixed;bottom: 0; width: 100%; background-color: #ffffff;border-top: 1px solid #E6E6E6;z-index: 3}
/* .pre{height: 84rpx;width: 25%;line-height: 84rpx; border: 1px solid #A6A6A6;text-align: center;margin: 20rpx} */
.done{height: 84rpx;line-height: 84rpx;width: 100%;text-align: center;background-color: #F66F36;margin: 20rpx 20rpx 20rpx 20rpx;color: #ffffff}
.dealstyle{position: fixed;bottom: 0; z-index: 100}
.top{height: 130rpx;line-height: 130rpx; color: #888989;background-color: #F4F6F7;text-align: center;border-top-left-radius: 20rpx;border-top-right-radius: 20rpx;}
.style-wrapper{position: relative; background-color: #ffffff;}
.style-wrapper::after{position: absolute;content: '';-webkit-transform-origin: 0 0;transform-origin: 0 0;-webkit-transform: scaleY(0.5);transform: scaleY(0.5);bottom: 0;right: 0;width: 100%;border-bottom: 1px solid #E6E6E6;}
.dealtitle{padding: 40rpx 40rpx 20rpx 40rpx;font-weight: 600;}
.dealcon{padding: 0 40rpx 40rpx 40rpx;}
.iconduihao{color: #F66F36;margin-right: 40rpx}
.new{color: #F66F36;margin-top: 20rpx;margin-left: -20rpx; font-size: 24rpx;font-weight: 700}
.start,.end{position: fixed;bottom: 0; width: 100%; height: 300px;z-index: 100}
.seleted{height: 50px;background-color: rgba(255,255,255,0.5)}
.shadow{position: absolute;width: 100%;top: 0;bottom: 0;height: 100vh;z-index: 1;background: rgba(0,0,0,.5);}
.name,.descr,.addimg{margin-left: 40rpx}
.bottomtips{position: relative;width: 100%;text-align: center;margin-top: 40rpx}
