page{background-color: #F1F1F1;padding-bottom: 400rpx}
.loadinggif{width: 80rpx;height: 80rpx;margin-bottom: 40rpx;color: #666666}
.mask{position: absolute;z-index: 9;font-size: 32rpx;width: 100vw;height: 100vh;background-color: #F1F1F1;top:100rpx;color: #666666}
.iconkong{font-size: 200rpx;margin: 80rpx auto;color: #666666}
.kong{margin-bottom: 20rpx;}
.title{padding: 40rpx 40rpx 0 40rpx;font-weight: 300;font-size: 38rpx}
.card{background-color: #ffffff;border-radius: 10rpx;margin:40rpx 20rpx 0 20rpx;overflow: hidden}
.img{width: 100%;height: 456rpx}
.info{margin: 30rpx}
.name{font-weight: 700;overflow:hidden; text-overflow:ellipsis;display:-webkit-box; -webkit-box-orient:vertical;-webkit-line-clamp:2; }
.price{color: #F15917;font-weight: 800;padding-bottom: 10rpx}
.endtitle{padding: 40rpx 40rpx 40rpx 40rpx;font-weight: 300;font-size: 38rpx}
.endlist-wrapper{margin:0 20rpx;border-radius: 10rpx;overflow: hidden}
.endlist{border-bottom: 1px solid #D9D9D9}
.img2{height: 140rpx;width: 140rpx;margin: 30rpx}
.endname{margin-bottom: 40rpx;overflow:hidden; text-overflow:ellipsis;display:-webkit-box; -webkit-box-orient:vertical;-webkit-line-clamp:2;}
.liupai{background-color: #cccccc;color: #fff;width: 100rpx;height: 40rpx;line-height: 40rpx;text-align: center;border-radius: 10rpx;}



